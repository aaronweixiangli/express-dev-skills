<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <style>
      body {
        align-items: flex-start;
      }
    </style>
  </head>
  <body>
    <%- include('../../partials/header') %>
    <a href="/">Home</a>
    <h1><%= title %></h1>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          <a href="/skills/<%= s.id %>"><%= s.name %></a>
        </li>
      <% }); %>
    </ul>
    <%- include('../../partials/footer') %>
  </body>
</html>
